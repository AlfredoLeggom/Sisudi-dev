extends ../templates/template

block config
    -actual_page = "insumos"

    //-Importación de bloques
    include ../blocks/block-menu
    include ../blocks/block-product-insumos
    //-include ../blocks/block-moda
    include ../blocks/block-footer
    //-Importar configuraciones extras
    include ../config/config-product

block append description 
    meta(name="description" content="Conoce nuestros productos e insumos médicos por la contingencia, recuerda cuidar tu salud y de los que te rodean.")

block header
    //Inicio de menu
    +block-menu(Categories, SubCategories, CategoriesIcon, url_mainLogo)
    //Fin de menu
    
block container
    //Inicio de insumos
    +block-product-insumos(insumosProductData, insumosDataPage)
    //Fin de insumos

block footer
    //Inicio de Footer
    +block-footer(title_page, categorias, subCategorias, nameCategorias, nameSubCategorias)
    //Fin de Footer